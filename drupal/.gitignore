# Ignore directories generated by Composer
/.editorconfig
/.gitattributes
/vendor/
/web/_ping.php
/web/.gitignore
/web/core/
/web/profiles/contrib/
/web/libraries/
/web/**/.gitignore
/drush/Commands/
/web/*/contrib/
/web/sites/default/default.*
/web/sites/example.*
/web/**/README.txt

# Ignore individual files added by the drupal/core-composer-scaffold composer plugin:
/web/.csslintrc
/web/.eslintignore
/web/.eslintrc.json
/web/.ht.router.php
/web/.htaccess
/web/autoload.php
/web/example.gitignore
/web/index.php
/web/INSTALL.txt
/web/README.md
/web/robots.txt
/web/update.php
/web/web.config

# Ignore local configuration
/web/sites/*/settings.local.php

# Ignore Drupal's file directory
/web/sites/default/files/**/*.jpg
/web/sites/default/files/**/*.jpeg
/web/sites/default/files/**/*.png
/web/sites/default/files/**/*.htaccess
/web/sites/default/files/**/*.gif
/web/sites/default/files/*.xml
/web/sites/default/files/php/twig

# Alowing images related to service and service category content types
!/web/sites/default/files/**/service_*.*


# Ignore testing directories.
/web/sites/simpletest
/web/tmp
